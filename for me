simvoli=['~','`','!','@','"','#','№','$',';','%','^',':','&','?','*','(',')','-','+',]

def avtoris(d):
  l=input('Введите ваш логин: ')
  p=input('Ведите пароль от вашего логина: ')
  for j in d:
    if j[0]==l:
      if j[1]==p:
        print('Вы успешно авторизовались')
      else:
        print('Логин или пароль введён неверно, попробуйте ещё раз')

